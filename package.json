{
  "name": "certlab",
  "version": "2.0.0",
  "description": "A modern, cloud-based certification study platform for CISSP, CISM, and more with adaptive quizzes, achievements, and progress tracking",
  "keywords": [
    "certification",
    "cissp",
    "cism",
    "security-plus",
    "learning",
    "quiz",
    "education",
    "firebase",
    "react",
    "typescript"
  ],
  "author": "archubbuck",
  "repository": {
    "type": "git",
    "url": "https://github.com/archubbuck/certlab.git"
  },
  "bugs": {
    "url": "https://github.com/archubbuck/certlab/issues"
  },
  "homepage": "https://github.com/archubbuck/certlab#readme",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:strict": "tsc && vite build",
    "build:firebase": "cross-env VITE_BASE_PATH=/ vite build",
    "preview": "vite preview",
    "check": "tsc",
    "check:firebase": "node scripts/check-firebase-config.js",
    "check:dynatrace": "node scripts/check-dynatrace-config.js",
    "lint": "eslint client/src shared",
    "lint:fix": "eslint client/src shared --fix",
    "format": "prettier --write \"client/src/**/*.{ts,tsx}\" \"shared/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"client/src/**/*.{ts,tsx}\" \"shared/**/*.{ts,tsx}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "deploy:firebase": "npm run build:firebase && firebase deploy --only hosting",
    "deploy:firestore:rules": "firebase deploy --only firestore:rules",
    "deploy:firestore:indexes": "firebase deploy --only firestore:indexes",
    "deploy:firestore": "firebase deploy --only firestore",
    "deploy:all": "npm run build:firebase && firebase deploy",
    "emulators:start": "firebase emulators:start",
    "emulators:exec": "firebase emulators:exec",
    "dynatrace:snippet": "node scripts/generate-dynatrace-snippet.js",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
